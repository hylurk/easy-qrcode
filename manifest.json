{
  "name": "Easy QRCode",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "kabulore@gmail.com",
  "description": "当前网址生成二维码",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_title": "生成当前网址二维码",
    "default_popup": "browser/popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}